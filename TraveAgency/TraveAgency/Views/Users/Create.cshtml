@model AppUser
<div class="card-body">
    <div class="card-header d-flex justify-content-between align-items-center">
        <h5 class="mb-0">User Create</h5>
    </div>
    <form method="post" enctype="multipart/form-data">


        <div class="form-group">
            <label class="form-label" asp-for="Name">  Name</label>
            <input asp-for="Name" type="text" class="form-control text-uppercase">
            <span class="text-danger" asp-validation-for="Name"></span>
        </div>

        <div class="form-group">
            <label class="form-label" asp-for="SurName"></label>
            <input asp-for="SurName" type="text" class="form-control text-uppercase">
            <span class="text-danger" asp-validation-for="SurName"></span>
        </div>
        
     
        <div class="form-group">
            <label class="form-label" asp-for="PhoneNumber"></label>
            <input asp-for="PhoneNumber" type="text" class="form-control ">
            <span class="text-danger" asp-validation-for="PhoneNumber"></span>
        </div>

        <div class="form-group">
            <label class="form-label" asp-for="Email"></label>
            <input asp-for="Email" type="text" class="form-control ">
            <span class="text-danger" asp-validation-for="Email"></span>
        </div>

          <div class="form-group">
            <label class="form-label" asp-for="UserName"></label>
            <input asp-for="UserName" type="text" class="form-control ">
            <span class="text-danger" asp-validation-for="UserName"></span>
        </div>
        
          <div class="form-group">
            <label class="form-label" asp-for="PasswordHash"></label>
            <input asp-for="PasswordHash" type="text" class="form-control ">
            <span class="text-danger" asp-validation-for="PasswordHash"></span>
        </div>
        <a asp-action="index" class="btn btn-dark mr-2">Go Back</a>
        <button type="submit" class="btn btn-primary mr-2">Send</button>
    </form>
</div>
@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}