<!DOCTYPE html>
<html lang="en">
<!-- index.html  21 Nov 2019 03:44:50 GMT -->
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Otika - Admin Dashboard Template</title>
    <!-- General CSS Files -->
    <link rel="stylesheet" href="~/assets/css/app.min.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/components.css">
    <!-- Custom style CSS -->
    <link rel="stylesheet" href="~/assets/css/custom.css">
    <link rel='shortcut icon' type='image/x-icon' href='~/assets/img/favicon.ico' />
    @await RenderSectionAsync("Css",false);
</head>

<body>
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar sticky">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg
									collapse-btn"> <i data-feather="align-justify"></i></a>
                        </li>
                        <li>
                            <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                                <i data-feather="maximize"></i>
                            </a>
                        </li>
                        <li>
                            <form class="search-form d-none d-md-block" action="#">
                                <i class="icon-magnifier"></i>
                                <input type="search" class="form-control" placeholder="Search Here" title="Search here">
                            </form>
                        </li>
                     </ul>
                </div>
                <ul class="navbar-nav navbar-right">
                    <li>
                        <a>
                            @User.Identity.Name
                        </a>
                        <br />
                        <a asp-controller="Home" asp-action="Logout">
                            Logout
                        </a>

                    </li>
                </ul>
            </nav>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="index.html">
                            <img alt="image" src="~/assets/img/logo.png" class="header-logo" /> <span class="logo-name">Otika</span>
                        </a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Main</li>
                        <li class="dropdown active">
                            <a asp-controller="Home" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Home</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="HotelCategories" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>HotelCategories</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="HotelRoomTypes" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>HotelRoomTypes</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Hotels" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Hotels</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Airports" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Airports</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="SeatClasses" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Seat Classes</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="AirlineTickets" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Airline Tickets</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="TourCategories" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Tour Categories</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Tours" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Tours</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Positions" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Positions</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Employees" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Employees</span></a>
                        </li>

                        <li class="dropdown active">
                            <a asp-controller="Users" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Users</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Incomes" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Incomes</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="Expenses" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>Expenses</span></a>
                        </li>
                        <li class="dropdown active">
                            <a asp-controller="PaidedSalaries" asp-action="Index" class="nav-link"><i data-feather="grid"></i><span>PaidedSalaries</span></a>
                        </li>
                    </ul>
                </aside>
            </div>
            <!-- Main Content -->
            <div class="main-content">
                @RenderBody()
            </div>
            <footer class="main-footer">
               
            </footer>
        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="~/assets/js/app.min.js"></script>
    <!-- JS Libraies -->
    <script src="~/assets/bundles/apexcharts/apexcharts.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/assets/js/page/index.js"></script>
    <!-- Template JS File -->
    <script src="~/assets/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="~/assets/js/custom.js"></script>
    @await RenderSectionAsync("Scripts",false)
</body>


<!-- index.html  21 Nov 2019 03:47:04 GMT -->
</html>
